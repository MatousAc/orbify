<xh:html xmlns:xh="http://www.w3.org/1999/xhtml" xmlns:xf="http://www.w3.org/2002/xforms"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ev="http://www.w3.org/2001/xml-events"
         xmlns:xi="http://www.w3.org/2001/XInclude"
         xmlns:xxi="http://orbeon.org/oxf/xml/xinclude"
         xmlns:xxf="http://orbeon.org/oxf/xml/xforms"
         xmlns:map="http://www.w3.org/2005/xpath-functions/map"
         xmlns:array="http://www.w3.org/2005/xpath-functions/array"
         xmlns:exf="http://www.exforms.org/exf/1-0"
         xmlns:fr="http://orbeon.org/oxf/xml/form-runner"
         xmlns:saxon="http://saxon.sf.net/"
         xmlns:sql="http://orbeon.org/oxf/xml/sql"
         xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
         xmlns:fb="http://orbeon.org/oxf/xml/form-builder"
         fr:data-format-version="4.0.0">
    <xh:head>
        <xh:title>IT Request Contact Information</xh:title>
        <xf:model id="fr-form-model" xxf:expose-xpath-types="true" xxf:analysis.calculate="true">

            
            <xf:instance id="fr-form-instance" xxf:exclude-result-prefixes="#all" xxf:index="id">
                <form>
                    <contact_information>
                        <grid-1>
                            <control-1 filename="FTD logo Full Color.jpg" mediatype="image/jpeg">/fr/service/persistence/crud/orbeon/builder/data/f3b1932efed53c4d9345c74c8205ac6b8da5c91a/a7199ec5951beb7002622bd4456b1063208a8b42.bin</control-1>
                            <employee_first_name/>
                            <employee_last_name/>
                            <is_new_employee/>
                            <employee_email_address/>
                            <employee_cell_phone_number/>
                            <shipping_address/>
                            <employee_start_date/>
                        </grid-1>
                        
                    </contact_information>
                </form>
            </xf:instance>

            
            <xf:bind id="fr-form-binds" ref="instance('fr-form-instance')">
                <xf:bind id="contact_information-bind" name="contact_information"
                         ref="contact_information">
                    <xf:bind id="grid-1-bind" ref="grid-1" name="grid-1">
                        <xf:bind id="control-1-bind" ref="control-1" name="control-1" type="xf:anyURI"/>
                        <xf:bind id="employee_first_name-bind" ref="employee_first_name"
                                 name="employee_first_name"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="employee_last_name-bind" ref="employee_last_name" name="employee_last_name"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="shipping_address-bind" ref="shipping_address" name="shipping_address"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="employee_cell_phone_number-bind" ref="employee_cell_phone_number"
                                 name="employee_cell_phone_number"
                                 xxf:whitespace="trim"
                                 required="true()"
                                 constraint=". = '' or matches(., '^\+?(\s*\(?\d\D?\s*){10,13}$')"
                                 calculate="if (string-length(replace(., '\D', '')) = 10)&#xA;then replace(replace(., '\D', ''),&#xA;        '(\d{3})(\d{3})(\d{4})', '($1) $2-$3')&#xA;else if (string-length(replace(., '\D', '')) = 11)&#xA;then replace(replace(., '\D', ''),&#xA;        '(\d{1})(\d{3})(\d{3})(\d{4})', '+$1 ($2) $3-$4')&#xA;else if (string-length(replace(., '\D', '')) = 12)&#xA;then replace(replace(., '\D', ''),&#xA;        '(\d{2})(\d{3})(\d{3})(\d{4})', '+$1 ($2) $3-$4')&#xA;else if (string-length(replace(., '\D', '')) = 13)&#xA;then replace(replace(., '\D', ''),&#xA;        '(\d{3})(\d{3})(\d{3})(\d{4})', '+$1 ($2) $3-$4')&#xA;else ."
                                 readonly="false()"/>
                        <xf:bind id="employee_email_address-bind" ref="employee_email_address"
                                 name="employee_email_address"
                                 type="xf:email"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="is_new_employee-bind" ref="is_new_employee" name="is_new_employee"
                                 type="xs:boolean"
                                 required="true()"/>
                        <xf:bind id="employee_start_date-bind" ref="employee_start_date"
                                 name="employee_start_date"
                                 type="xf:date"
                                 relevant="$is_new_employee/string() = 'true'"
                                 required="$is_new_employee/string() = 'true'"/>
                    </xf:bind>
                    
                </xf:bind>
            </xf:bind>

            
            <xf:instance id="fr-form-metadata" xxf:readonly="true" xxf:exclude-result-prefixes="#all">
                <metadata>
                    <application-name>ftd</application-name>
                    <form-name>it_request_contact_information</form-name>
                    <title xml:lang="en">IT Request Contact Information</title>
                    <description xml:lang="en"/>
                    <created-with-version>2019.2.2.202003311635 PE</created-with-version>
                    <updated-with-version>2019.2.2.202003311635 PE</updated-with-version>
                </metadata>
            </xf:instance>

            
            <xf:instance id="fr-form-attachments" xxf:exclude-result-prefixes="#all">
                <attachments/>
            </xf:instance>

            
            <xf:instance xxf:readonly="true" id="fr-form-resources" xxf:exclude-result-prefixes="#all">
                <resources>
                    <resource xml:lang="en">
                        <control-1>
                            <label/>
                        </control-1>
                        <employee_first_name>
                            <label>Employee First Name</label>
                            <hint/>
                        </employee_first_name>
                        <employee_last_name>
                            <label>Employee Last Name</label>
                            <hint/>
                        </employee_last_name>
                        <is_new_employee>
                            <label>New Employee?</label>
                            <hint/>
                        </is_new_employee>
                        <employee_email_address>
                            <label>Employee Email Address</label>
                            <hint/>
                        </employee_email_address>
                        <employee_cell_phone_number>
                            <label>Employee Cell Phone Number</label>
                            <hint/>
                        </employee_cell_phone_number>
                        <shipping_address>
                            <label>Shipping Address</label>
                            <hint/>
                        </shipping_address>
                        <employee_start_date>
                            <label>Employee Start Date</label>
                            <hint/>
                        </employee_start_date>
                        <contact_information>
                            <label>Contact Information</label>
                        </contact_information>
                        
                    </resource>
                </resources>
            </xf:instance>

        </xf:model>
    </xh:head>
    <xh:body>
        <fr:view>
            <fr:body xmlns:xbl="http://www.w3.org/ns/xbl" xmlns:p="http://www.orbeon.com/oxf/pipeline"
                     xmlns:oxf="http://www.orbeon.com/oxf/processors">
                <fr:section id="contact_information-section" bind="contact_information-bind">
                    <fr:grid id="grid-1-grid" bind="grid-1-bind">
                        <fr:c y="1" x="1" w="6">
                            <fr:image xmlns="http://orbeon.org/oxf/xml/form-builder"
                                      xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                      id="control-1-control"
                                      class="fr-static-attachment"
                                      bind="control-1-bind">
                                <xf:label ref="$form-resources/control-1/label"/>
                         
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                            </fr:image>
                                
                            </fr:c>
                        <fr:c x="7" y="1" w="6" h="1"/>
                        <fr:c x="1" y="2" w="12">
                            <xf:input id="employee_first_name-control" bind="employee_first_name-bind">
                                <xf:label ref="$form-resources/employee_first_name/label"/>
                                <xf:hint ref="$form-resources/employee_first_name/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="3" w="12">
                            <xf:input id="employee_last_name-control" bind="employee_last_name-bind">
                                <xf:label ref="$form-resources/employee_last_name/label"/>
                                <xf:hint ref="$form-resources/employee_last_name/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="4" w="12">
                            <fr:yesno-input xmlns="http://orbeon.org/oxf/xml/form-builder"
                                            xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                            id="is_new_employee-control"
                                            bind="is_new_employee-bind">
                                <xf:label ref="$form-resources/is_new_employee/label"/>
                                <xf:hint ref="$form-resources/is_new_employee/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </fr:yesno-input>
                        </fr:c>
                        <fr:c x="1" y="5" w="12">
                            <xf:input id="employee_email_address-control" bind="employee_email_address-bind">
                                <xf:label ref="$form-resources/employee_email_address/label"/>
                                <xf:hint ref="$form-resources/employee_email_address/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="6" w="12">
                            <xf:input id="employee_cell_phone_number-control" bind="employee_cell_phone_number-bind">
                                <xf:label ref="$form-resources/employee_cell_phone_number/label"/>
                                <xf:hint ref="$form-resources/employee_cell_phone_number/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="7" w="12">
                            <xf:input id="shipping_address-control" bind="shipping_address-bind">
                                <xf:label ref="$form-resources/shipping_address/label"/>
                                <xf:hint ref="$form-resources/shipping_address/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="8" w="12">
                            <fr:date xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                     xmlns:DateSupport="java:org.orbeon.xbl.DateSupportJava"
                                     id="employee_start_date-control"
                                     bind="employee_start_date-bind">
                                <xf:label ref="$form-resources/employee_start_date/label"/>
                                <xf:hint ref="$form-resources/employee_start_date/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                        
                        
                            </fr:date>
                        </fr:c>
                    </fr:grid>
                    <xf:label ref="$form-resources/contact_information/label"/>
                    
                </fr:section>
            </fr:body>
        </fr:view>
    </xh:body>
</xh:html>