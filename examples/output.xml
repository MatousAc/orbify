	<xh:head>
		<xh:title>iqr</xh:title>
		<xf:model id="fr-form-model" xxf:expose-xpath-types="true" xxf:analysis.calculate="true">
			<xf:instance id="fr-form-instance" xxf:exclude-result-prefixes="#all" xxf:index="id">
				<form>
					<section_0>
						<grid_0>
							<is_qir_required/>
						</grid_0>
					</section_0>
					<section_1>
						<grid_1>
							<J/>
							<qir_title/>
							<describe_qir/>
						</grid_1>
					</section_1>
					<project_systematization>
						<grid_2>
							<Y/>
							<is_can_project_be_systematized/>
							<aspects_to_systematize/>
							<O/>
							<describe_systemztization/>
						</grid_2>
					</project_systematization>
					<section_2>
						<grid_3>
							<C/>
							<qir_initiated_for_lessons_learned/>
							<I/>
							<qir_initiated_for_systemazation/>
						</grid_3>
					</section_2>
				</form>
			</xf:instance>
			<xf:bind id="fr-form-binds" ref="instance('fr-form-instance')">
				<xf:bind id="section_0-bind" ref="section_0" name="section_0">
					<xf:bind id="grid_0-bind" ref="grid_0" name="grid_0">
						<xf:bind id="is_qir_required-bind" ref="is_qir_required" name="is_qir_required" type="xf:boolean"/>
</xf:bind>
				</xf:bind>
				<xf:bind id="section_1-bind" ref="section_1" name="section_1">
					<xf:bind id="grid_1-bind" ref="grid_1" name="grid_1">
						<xf:bind id="J-bind" ref="J" name="J"/>
						<xf:bind id="qir_title-bind" ref="qir_title" name="qir_title" xxf:whitespace="trim"/>
						<xf:bind id="describe_qir-bind" ref="describe_qir" name="describe_qir" xxf:whitespace="trim"/>
</xf:bind>
				</xf:bind>
				<xf:bind id="project_systematization-bind" ref="project_systematization" name="project_systematization">
					<xf:bind id="grid_2-bind" ref="grid_2" name="grid_2">
						<xf:bind id="Y-bind" ref="Y" name="Y"/>
